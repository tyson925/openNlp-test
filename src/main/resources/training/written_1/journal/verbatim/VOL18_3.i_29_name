<ENAMEX TYPE="PERSON">sh≈´shannƒÅh</ENAMEX>, `<ENAMEX TYPE="PLANT">lily</ENAMEX>', see <ENAMEX TYPE="ORGANIZATION">SUSANNA</ENAMEX>, <ENAMEX TYPE="ORGANIZATION">SUSANNAH</ENAMEX>.  <ENAMEX TYPE="ORGANIZATION">SUSAN</ENAMEX>
<ENAMEX TYPE="WORK_OF_ART">IS CHARACTER IN THOMAS HEYWOOD'S PLAY A</ENAMEX>
