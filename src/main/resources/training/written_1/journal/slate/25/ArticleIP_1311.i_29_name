<ENAMEX TYPE="NATIONALITY">American</ENAMEX> life nowadays" (<ENAMEX TYPE="PERSON">Richard Schickel</ENAMEX>, <ENAMEX TYPE="ORGANIZATION">Time</ENAMEX> ). <ENAMEX TYPE="PERSON">Dissenting</ENAMEX>, <ENAMEX TYPE="WORK_OF_ART">The New</ENAMEX>
