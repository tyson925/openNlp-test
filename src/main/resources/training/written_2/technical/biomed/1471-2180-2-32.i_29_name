          <ENAMEX TYPE="ORGANIZATION">Plasmids</ENAMEX> <ENAMEX TYPE="PRODUCT">pKL106</ENAMEX> and <ENAMEX TYPE="PRODUCT">pKL107</ENAMEX> <ENAMEX TYPE="PRODUCT_DESC">encoding</ENAMEX> <ENAMEX TYPE="PRODUCT">LA57-N 102LT-FLAG</ENAMEX>
