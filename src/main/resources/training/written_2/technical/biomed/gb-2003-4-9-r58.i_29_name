        <ENAMEX TYPE="ORGANIZATION">Shal</ENAMEX> ( 
        <ENAMEX TYPE="ORGANIZATION">Dm</ENAMEX> <ENAMEX TYPE="PRODUCT">3L</ENAMEX>, 
        <ENAMEX TYPE="ORGANIZATION">Ag 2L</ENAMEX>), 
        <ENAMEX TYPE="ORGANIZATION">KCNQ</ENAMEX> ( 
        <ENAMEX TYPE="ORGANIZATION">Dm</ENAMEX> <ENAMEX TYPE="PRODUCT">2R</ENAMEX>, 
