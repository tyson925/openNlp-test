          <ENAMEX TYPE="ORGANIZATION">N</ENAMEX> ) = <NUMEX TYPE="MONEY">σ</NUMEX> (<NUMEX TYPE="MONEY">25</NUMEX>, 
          <ENAMEX TYPE="ORGANIZATION">N</ENAMEX> ) {<NUMEX TYPE="CARDINAL">1.000</NUMEX>-<NUMEX TYPE="CARDINAL">1.962</NUMEX> × <NUMEX TYPE="PERCENT">10 -2Δ</NUMEX> + <NUMEX TYPE="CARDINAL">8.08</NUMEX> ×
          <ENAMEX TYPE="CONTACT_INFO">10 -5Δ 2</ENAMEX>
          - 
          <ENAMEX TYPE="ORGANIZATION">N Δ</ENAMEX> [<NUMEX TYPE="QUANTITY">3.020 × 10 -5+ 3.922 × 10 -5Δ</NUMEX>
          <ENAMEX TYPE="CONTACT_INFO">+</ENAMEX> 
          <ENAMEX TYPE="ORGANIZATION">N</ENAMEX> (<NUMEX TYPE="QUANTITY">1.721 × 10 -5Δ</NUMEX>
          - <NUMEX TYPE="CARDINAL">6.584</NUMEX> × <NUMEX TYPE="PERCENT">10 -6Δ</NUMEX>)]} (<NUMEX TYPE="MONEY">Eq.1</NUMEX>)
          where
          <ENAMEX TYPE="ORGANIZATION">σ</ENAMEX> (<NUMEX TYPE="MONEY">25</NUMEX>, 
          <ENAMEX TYPE="ORGANIZATION">N</ENAMEX> ) = 
          <ENAMEX TYPE="ORGANIZATION">N</ENAMEX> [<NUMEX TYPE="CARDINAL">10.394</NUMEX>-<NUMEX TYPE="CARDINAL">2.3776</NUMEX> 
          <ENAMEX TYPE="ORGANIZATION">N</ENAMEX> +0.68258 
          <ENAMEX TYPE="ORGANIZATION">N</ENAMEX> <NUMEX TYPE="CARDINAL">2</NUMEX>- <NUMEX TYPE="CARDINAL">9.13538</NUMEX> 
          <ENAMEX TYPE="ORGANIZATION">N</ENAMEX> <NUMEX TYPE="CARDINAL">3</NUMEX>+ <NUMEX TYPE="CARDINAL">1.0086</NUMEX> × <NUMEX TYPE="PERCENT">10 -2</NUMEX> 
