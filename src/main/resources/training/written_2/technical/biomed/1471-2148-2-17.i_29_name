        <ENAMEX TYPE="ORGANIZATION">Asteraceae</ENAMEX> [ <TIMEX TYPE="DATE">61</TIMEX> ] , for <ENAMEX TYPE="ORGANIZATION">Tribe Anthemideae</ENAMEX> specifically [ <NUMEX TYPE="CARDINAL">62</NUMEX>
        <NUMEX TYPE="CARDINAL">63</NUMEX> ] and within 
        <ENAMEX TYPE="ORGANIZATION">Artemisia</ENAMEX> [ <NUMEX TYPE="CARDINAL">34 36</NUMEX> ] .
      
      
