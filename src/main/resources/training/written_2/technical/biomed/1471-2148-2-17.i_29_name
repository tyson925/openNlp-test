        of A<ENAMEX TYPE="PER_DESC">-C</ENAMEX> = <NUMEX TYPE="CARDINAL">1.0000</NUMEX>, A-<ENAMEX TYPE="ORGANIZATION">G</ENAMEX> = <NUMEX TYPE="CARDINAL">3.1857</NUMEX>, A<ENAMEX TYPE="PER_DESC">-T</ENAMEX> = <NUMEX TYPE="CARDINAL">1.0000</NUMEX>, <ENAMEX TYPE="ORGANIZATION">C-G</ENAMEX> = <NUMEX TYPE="CARDINAL">1.0000</NUMEX>,
