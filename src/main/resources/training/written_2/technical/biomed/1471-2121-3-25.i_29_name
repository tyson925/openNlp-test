        skin <ENAMEX TYPE="DISEASE">pigmentation</ENAMEX> and <ENAMEX TYPE="PRODUCT">T-</ENAMEX><ENAMEX TYPE="PRODUCT_DESC">lymphocyte</ENAMEX> and macrophage
