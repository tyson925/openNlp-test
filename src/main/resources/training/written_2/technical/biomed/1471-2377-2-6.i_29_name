        <ENAMEX TYPE="PERSON">level ></ENAMEX> = <NUMEX TYPE="CARDINAL">3</NUMEX> of <NUMEX TYPE="PERCENT">8%</NUMEX> (<NUMEX TYPE="PERCENT">95%</NUMEX> <ENAMEX TYPE="ORGANIZATION">CI</ENAMEX> <ENAMEX TYPE="PRODUCT">6-9</ENAMEX>), after adjusting for age,
        gender and use of <ENAMEX TYPE="SUBSTANCE">antipsychotic medication</ENAMEX>.
