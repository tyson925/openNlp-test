          in <ENAMEX TYPE="LAW">vitro (≥ 25</ENAMEX> pM or <NUMEX TYPE="CARDINAL">0.8</NUMEX> <ENAMEX TYPE="PER_DESC">ng</ENAMEX>/ml) and
