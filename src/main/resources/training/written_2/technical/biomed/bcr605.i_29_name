          <ENAMEX TYPE="GPE">Massachusetts</ENAMEX>, <ENAMEX TYPE="GPE">Connecticut</ENAMEX>, <ENAMEX TYPE="GPE">New Jersey</ENAMEX>, <ENAMEX TYPE="GPE">New York</ENAMEX>,
          <ENAMEX TYPE="ORGANIZATION">Pennsylvania</ENAMEX>, <ENAMEX TYPE="GPE">Delaware</ENAMEX>, <ENAMEX TYPE="GPE">Washington</ENAMEX>, <ENAMEX TYPE="PRODUCT">DC</ENAMEX>, <ENAMEX TYPE="GPE">Rhode Island</ENAMEX>, and
          <ENAMEX TYPE="ORGANIZATION">Maryland</ENAMEX>); <ENAMEX TYPE="LOCATION">Great Lakes</ENAMEX> (<ENAMEX TYPE="GPE">Minnesota</ENAMEX>, <ENAMEX TYPE="GPE">Iowa</ENAMEX>, <ENAMEX TYPE="GPE">Illinois</ENAMEX>,
          <ENAMEX TYPE="ORGANIZATION">Wisconsin</ENAMEX>, <ENAMEX TYPE="GPE">Michigan</ENAMEX>, <ENAMEX TYPE="GPE">Indiana</ENAMEX>, and <ENAMEX TYPE="GPE">Ohio</ENAMEX>); <ENAMEX TYPE="GPE">Plains</ENAMEX> (<ENAMEX TYPE="ORGANIZATION">North</ENAMEX>
