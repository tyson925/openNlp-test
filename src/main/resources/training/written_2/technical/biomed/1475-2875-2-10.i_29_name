            <NUMEX TYPE="PERCENT">81%</NUMEX> stocked <ENAMEX TYPE="ORGANIZATION">AQ</ENAMEX>, while <NUMEX TYPE="PERCENT">only 39%</NUMEX> stocked <ENAMEX TYPE="PRODUCT">SP</ENAMEX> and <NUMEX TYPE="PERCENT">41%</NUMEX>
