          <ENAMEX TYPE="GPE">California</ENAMEX> <ENAMEX TYPE="GPE_DESC">counties</ENAMEX> (<ENAMEX TYPE="PRODUCT">Table 2</ENAMEX>). Among <ENAMEX TYPE="PER_DESC">women</ENAMEX> <TIMEX TYPE="DATE">age 45-64</TIMEX>
