          with <NUMEX TYPE="CARDINAL">four</NUMEX> <ENAMEX TYPE="PER_DESC">male patients</ENAMEX> and <NUMEX TYPE="CARDINAL">nine</NUMEX> female <ENAMEX TYPE="PER_DESC">patients</ENAMEX>. <NUMEX TYPE="CARDINAL">Nine</NUMEX>
