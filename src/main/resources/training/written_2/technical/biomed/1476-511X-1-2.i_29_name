            <ENAMEX TYPE="PER_DESC">hippocampus</ENAMEX> = <NUMEX TYPE="CARDINAL">1.36</NUMEX> + (90/min <ENAMEX TYPE="ORGANIZATION">ADI</ENAMEX>);
            LFC 
            liver = <NUMEX TYPE="CARDINAL">1.52</NUMEX>+(100/min <ENAMEX TYPE="ORGANIZATION">ADI</ENAMEX>). <ENAMEX TYPE="ORGANIZATION">Groups</ENAMEX>
