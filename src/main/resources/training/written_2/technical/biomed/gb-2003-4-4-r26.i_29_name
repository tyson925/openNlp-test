          <ENAMEX TYPE="ORGANIZATION">cDNA</ENAMEX> <ENAMEX TYPE="ORG_DESC">library</ENAMEX> (named <ENAMEX TYPE="ANIMAL">Bird_Rao</ENAMEX>_ 
