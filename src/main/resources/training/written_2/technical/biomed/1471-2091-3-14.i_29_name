          , <ENAMEX TYPE="CONTACT_INFO">TIF2/GRIP1/NcoA-2</ENAMEX>, [ <NUMEX TYPE="CARDINAL">94 95</NUMEX> ] and pCIP<ENAMEX TYPE="PRODUCT">/ACTR/AIB1</ENAMEX> [ <NUMEX TYPE="CARDINAL">96 97</NUMEX>
