          <ENAMEX TYPE="GPE">Idaho</ENAMEX>, <ENAMEX TYPE="GPE">Montana</ENAMEX>, <ENAMEX TYPE="GPE">Nevada</ENAMEX>, <ENAMEX TYPE="GPE">New Mexico</ENAMEX>, <ENAMEX TYPE="GPE">Oregon</ENAMEX>, <ENAMEX TYPE="GPE">Utah</ENAMEX>,
